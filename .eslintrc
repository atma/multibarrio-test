{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true
  },
  "rules": {
    "indent": [2, 4],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2],
    "eol-last": [2]
  },
  "ecmaFeatures": {
    "modules": true,
    "restParams": true,
    "spread": true,
    "destructuring": true
  },
  "globals": {
    "tiny": false,
    "autocomplete": true
  }
}
